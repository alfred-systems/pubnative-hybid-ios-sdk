Engine: HomeOrWork
InputVariable: VisitDuration
  enabled: true
  range: 0.000 24.000
  lock-range: false
  term: Short Trapezoid -0.080 0.100 0.650 1.250
  term: Medium Trapezoid 0.650 1.250 3.500 4.000
  term: Long Trapezoid 3.500 4.000 24.000 24.000
InputVariable: DepartureTime
  enabled: true
  range: 0.000 25.000
  lock-range: false
  term: Morning Trapezoid 3.500 5.000 8.500 9.750
  term: Afternoon Trapezoid 10.000 10.750 17.000 18.000
  term: Evening Trapezoid 17.000 18.000 19.500 21.000
  term: Night Trapezoid 19.500 21.000 24.000 24.000
  term: DistantFuture Triangle 24.000 25.000 25.000
