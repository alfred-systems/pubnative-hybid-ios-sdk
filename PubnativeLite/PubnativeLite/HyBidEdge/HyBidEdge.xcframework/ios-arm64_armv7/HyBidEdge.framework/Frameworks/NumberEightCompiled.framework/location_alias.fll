RuleBlock: rule block 2
  enabled: true
  conjunction: AlgebraicProduct
  disjunction: Maximum
  implication: Minimum
  activation: General
  rule: if VisitDuration is Short and DepartureTime is Morning and WeekendOrWeekday is Weekday then Location is Home with 0.25
  rule: if VisitDuration is Short and DepartureTime is Morning and WeekendOrWeekday is Weekend then Location is Home with 0.5
  rule: if VisitDuration is Medium and DepartureTime is Morning then Location is Home with 0.75
  rule: if VisitDuration is Long and DepartureTime is Morning then Location is Home
  rule: if VisitDuration is Medium and DepartureTime is Night and WeekendOrWeekday is Weekend then Location is Home with 0.15
  rule: if VisitDuration is Long and DepartureTime is Night and WeekendOrWeekday is Weekend then Location is Home with 0.25
  rule: if VisitDuration is Short and DepartureTime is Night and WeekendOrWeekday is Weekday then Location is Home with 0.25
  rule: if VisitDuration is Medium and DepartureTime is Night and WeekendOrWeekday is Weekday then Location is Home with 0.33
  rule: if VisitDuration is Long and DepartureTime is Night and WeekendOrWeekday is Weekday then Location is Home with 0.5
  rule: if Residential is True then Location is Home with 0.25
  rule: if VisitDuration is Short and DepartureTime is Afternoon and WeekendOrWeekday is Weekday then Location is Work with 0.25
  rule: if VisitDuration is Medium and DepartureTime is Afternoon and WeekendOrWeekday is Weekday then Location is Work with 0.5
  rule: if VisitDuration is Long and DepartureTime is Afternoon and WeekendOrWeekday is Weekday then Location is Work
  rule: if VisitDuration is Long and DepartureTime is Evening and WeekendOrWeekday is Weekday then Location is Work